                                          <mn>0.135</mn>
                                          <mspace width="0.2em"/>
                                          <mi>M</mi>
                                        </mrow>
                                      </mrow>
                                      <annotation-xml encoding="MathML-Content">
                                        <mrow>
                                          <mrow>
                                            <mo>[</mo>
                                            <mrow>
                                              <msub>
                                                <mrow>
                                                  <mtext>Cl</mtext>
                                                </mrow>
