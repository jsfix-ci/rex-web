                                              <mo>[</mo>
                                              <mrow>
                                                <mtext>NO</mtext>
                                              </mrow>
                                              <mo>]</mo>
                                            </mrow>
                                          </mrow>
                                          <mn>2</mn>
                                        </msup>
                                      </mrow>
