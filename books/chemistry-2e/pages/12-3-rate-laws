                                              <mrow>
                                                <mtext>rate</mtext>
                                              </mrow>
                                              <mi>y</mi>
                                            </msub>
                                          </mrow>
                                        </mfrac>
                                        <mspace width="0.1em"/>
                                        <mo>=</mo>
                                        <mspace width="0.1em"/>
                                        <mfrac>
                                          <mrow>
                                            <mi>k</mi>
                                            <msubsup>
                                              <mrow>
                                                <mtext>[</mtext>
                                                <mtext>NO</mtext>
