                              <p id="fs-idm272297280">
                                Canceling equivalent terms in the numerator and denominator leaves:
                                <span data-type="newline">
                                  <br>
                                </span>
                              </p>
                              <div data-type="equation" id="fs-idm185610624">
                                <math display="block">
                                  <semantics>
                                    <mrow>
                                      <mrow>
                                        <mfrac>
                                          <mrow>
                                            <mn>0.00675</mn>
                                          </mrow>
                                          <mrow>
                                            <mn>0.00300</mn>
                                          </mrow>
                                        </mfrac>
                                        <mspace width="0.1em"/>
                                        <mo>=</mo>
                                        <mspace width="0.1em"/>
                                        <mfrac>
                                          <mrow>
                                            <msup>
                                              <mrow>
                                                <mo stretchy="false">(</mo>
                                                <mn>0.15</mn>
                                                <mo stretchy="false">)</mo>
                                              </mrow>
                                              <mi>m</mi>
                                            </msup>
                                          </mrow>
                                          <mrow>
                                            <msup>
                                              <mrow>
                                                <mo stretchy="false">(</mo>
                                                <mn>0.10</mn>
                                                <mo stretchy="false">)</mo>
                                              </mrow>
                                              <mi>m</mi>
                                            </msup>
                                          </mrow>
                                        </mfrac>
                                      </mrow>
                                    </mrow>
                                    <annotation-xml encoding="MathML-Content">
                                      <mrow>
                                        <mfrac>
                                          <mrow>
                                            <mn>0.00675</mn>
                                          </mrow>
                                          <mrow>
                                            <mn>0.00300</mn>
                                          </mrow>
                                        </mfrac>
                                        <mspace width="0.1em"/>
                                        <mo>=</mo>
                                        <mspace width="0.1em"/>
                                        <mfrac>
                                          <mrow>
                                            <msup>
                                              <mrow>
                                                <mo stretchy="false">(</mo>
                                                <mn>0.15</mn>
                                                <mo stretchy="false">)</mo>
                                              </mrow>
                                              <mi>m</mi>
                                            </msup>
                                          </mrow>
                                          <mrow>
                                            <msup>
                                              <mrow>
                                                <mo stretchy="false">(</mo>
                                                <mn>0.10</mn>
                                                <mo stretchy="false">)</mo>
                                              </mrow>
                                              <mi>m</mi>
                                            </msup>
                                          </mrow>
                                        </mfrac>
                                      </mrow>
                                    </annotation-xml>
                                  </semantics>
                                </math>
                              </div>
