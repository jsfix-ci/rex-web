                                  <mi>g</mi>
                                  <mo stretchy="false">)</mo>
                                  <mspace width="0.2em"/>
                                  <mo stretchy="false">⟶</mo>
                                  <mspace width="0.2em"/>
                                  <msub>
                                    <mtext>2O</mtext>
                                    <mn>2</mn>
                                  </msub>
                                  <mo stretchy="false">(</mo>
