                                          <menclose notation="horizontalstrike">
                                            <mrow>
                                              <mtext>qt</mtext>
                                            </mrow>
                                          </menclose>
                                          <mspace width="0.2em"/>
                                          <mo>×</mo>
                                          <mspace width="0.2em"/>
                                          <mfrac>
                                            <mrow>
                                              <mtext>1 L</mtext>
                                            </mrow>
                                            <mrow>
                                              <mtext>1.0567</mtext>
                                              <mspace width="0.2em"/>
                                              <menclose
                                                notation="horizontalstrike"
                                              >
                                                <mrow>
                                                  <mtext>qt</mtext>
                                                </mrow>
                                              </menclose>
                                            </mrow>
                                          </mfrac>
