                            <math display="block">
                              <semantics>
                                <mrow>
                                  <mrow>
                                    <mtext>°F</mtext>
                                    <mo>=</mo>
                                    <mfrac>
                                      <mn>9</mn>
                                      <mn>5</mn>
                                    </mfrac>
                                    <mtext>°C</mtext>
                                    <mo>+</mo>
                                    <mn>32.0</mn>
                                    <mo>=</mo>
                                    <mrow>
                                      <mo>(</mo>
                                      <mrow>
                                        <mfrac>
                                          <mn>9</mn>
                                          <mn>5</mn>
                                        </mfrac>
                                        <mspace width="0.2em"/>
                                        <mo>×</mo>
                                        <mspace width="0.2em"/>
                                        <mn>37.0</mn>
                                      </mrow>
                                      <mo>)</mo>
                                    </mrow>
                                    <mo>+</mo>
                                    <mn>32.0</mn>
                                    <mo>=</mo>
                                    <mn>66.6</mn>
                                    <mo>+</mo>
                                    <mn>32.0</mn>
                                    <mo>=</mo>
                                    <mtext>98.6 °F</mtext>
                                  </mrow>
                                </mrow>
