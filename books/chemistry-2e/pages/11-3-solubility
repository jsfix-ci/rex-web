                                          </mrow>
                                        </msup>
                                        <mspace width="0.2em"/>
                                        <mo>×</mo>
                                        <mspace width="0.2em"/>
                                        <mn>20.7</mn>
                                        <mspace width="0.2em"/>
                                        <mtext>kPa</mtext>
                                      </mtd>
                                    </mtr>
                                    <mtr>
                                      <mtd/>
                                      <mtd/>
                                      <mtd>
