<mtr><mtd/><mtd columnalign="left"><mo>=</mo></mtd><mtd columnalign="left"><mn>+0.46</mn><mspace width="0.2em"/><mtext>V</mtext></mtd></mtr></mtable></mrow><annotation-xml encoding="MathML-Content"><mtable><mtr><mtd columnalign="right"><msup><mtext>E</mtext><mtext>°</mtext></msup><msub><mrow/><mrow><mtext>cell</mtext></mrow></msub></mtd><mtd columnalign="left"><mo>=</mo></mtd><mtd columnalign="left"><msup><mtext>E</mtext><mtext>°</mtext></msup><msub><mrow/><mrow><mtext>cathode</mtext></mrow></msub><mo>−</mo><msup><mtext>E</mtext><mtext>°</mtext></msup><msub><mrow/><mrow><mtext>anode</mtext></mrow></msub></mtd></mtr>
