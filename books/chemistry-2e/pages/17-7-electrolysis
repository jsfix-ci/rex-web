                                      <mo stretchy="false">)</mo>
                                      <mo>+</mo>
                                      <msup>
                                        <mtext>e</mtext>
                                        <mtext>−</mtext>
                                      </msup>
                                      <mspace width="0.2em"/>
                                      <mo stretchy="false">⟶</mo>
                                      <mspace width="0.2em"/>
                                      <mtext>Ag</mtext>
                                      <mo stretchy="false">(</mo>
                                      <mi>s</mi>
                                      <mo stretchy="false">)</mo>
                                    </mrow>
                                  </mrow>
                                  <annotation-xml encoding="MathML-Content">
                                    <mrow>
                                      <msup>
                                        <mrow>
                                          <mtext>cathode: Ag</mtext>
                                        </mrow>
