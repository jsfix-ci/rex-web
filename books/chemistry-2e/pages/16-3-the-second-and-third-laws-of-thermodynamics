                                      <mspace width="0.2em"/>
                                      <mfrac>
                                        <mrow>
                                          <mtext>−</mtext>
                                          <msub>
                                            <mi>q</mi>
                                            <mrow>
                                              <mtext>rev</mtext>
                                            </mrow>
                                          </msub>
                                        </mrow>
                                        <mrow>
                                          <msub>
