                                    <mo stretchy="false">(</mo>
                                    <mi>s</mi>
                                    <mo stretchy="false">)</mo>
                                    <mspace width="0.2em"/>
                                    <mo stretchy="false">⟶</mo>
                                    <mspace width="0.2em"/>
                                    <msub>
                                      <mtext>H</mtext>
                                      <mn>2</mn>
                                    </msub>
