                                            <mrow>
                                              <mtext>surr</mtext>
                                            </mrow>
                                          </msub>
                                        </mrow>
                                      </mfrac>
                                    </mrow>
                                  </mrow>
                                  <annotation-xml encoding="MathML-Content">
                                    <mrow>
                                      <mtext>Δ</mtext>
                                      <msub>
                                        <mi>S</mi>
                                        <mrow>
