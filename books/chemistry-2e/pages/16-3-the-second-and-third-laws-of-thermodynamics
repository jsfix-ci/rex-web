                                            <mrow>
                                              <mtext>Δ</mtext>
                                              <msub>
                                                <mi>S</mi>
                                                <mrow>
                                                  <mtext>sys</mtext>
                                                </mrow>
                                              </msub>
                                            </mrow>
                                            <mo>|</mo>
                                          </mrow>
                                          <mo>≈</mo>
                                          <mrow>
                                            <mo>|</mo>
                                            <mrow>
                                              <mtext>Δ</mtext>
                                              <msub>
                                                <mi>S</mi>
                                                <mrow>
                                                  <mtext>surr</mtext>
                                                </mrow>
                                              </msub>
                                            </mrow>
                                            <mo>|</mo>
                                          </mrow>
                                        </mtd>
                                      </mtr>
                                      <mtr>
                                        <mtd>
                                          <mtext>Δ</mtext>
                                          <msub>
                                            <mi>S</mi>
