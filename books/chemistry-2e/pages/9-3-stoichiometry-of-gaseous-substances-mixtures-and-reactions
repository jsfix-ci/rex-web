                                        <msub>
                                          <mtext>CO</mtext>
                                          <mn>2</mn>
                                        </msub>
                                        <mo stretchy="false">(</mo>
                                        <mi>g</mi>
                                        <mo stretchy="false">)</mo>
                                        <mo>+</mo>
                                        <mn>4</mn>
                                        <msub>
                                          <mtext>H</mtext>
                                          <mn>2</mn>
                                        </msub>
