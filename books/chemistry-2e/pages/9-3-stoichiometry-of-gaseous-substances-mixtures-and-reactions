                                        <msub>
                                          <mrow>
                                            <mtext>NH</mtext>
                                          </mrow>
                                          <mn>3</mn>
                                        </msub>
                                      </mrow>
                                    </menclose>
                                    <mspace width="0.2em"/>
                                    <mo>×</mo>
                                    <mspace width="0.2em"/>
                                    <mfrac>
                                      <mrow>
                                        <mtext>3 billion</mtext>
                                        <mspace width="0.2em"/>
                                        <msup>
                                          <mrow>
