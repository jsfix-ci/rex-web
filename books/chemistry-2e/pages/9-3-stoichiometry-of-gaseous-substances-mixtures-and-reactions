                                    </mfrac>
                                    <mo>=</mo>
                                    <mfrac>
                                      <mrow>
                                        <mo stretchy="false">(</mo>
                                        <mn>28.01</mn>
                                        <mspace width="0.2em"/>
                                        <mtext>g/mol</mtext>
                                        <mo stretchy="false">)</mo>
                                        <mo stretchy="false">(</mo>
                                        <mn>1.00</mn>
                                        <mspace width="0.2em"/>
                                        <mtext>atm</mtext>
                                        <mo stretchy="false">)</mo>
                                      </mrow>
                                      <mrow>
                                        <mo stretchy="false">(</mo>
                                        <mn>0.0821</mn>
                                        <mspace width="0.2em"/>
                                        <msup>
                                          <mrow>
                                            <mtext>L·atm·mol</mtext>
                                          </mrow>
