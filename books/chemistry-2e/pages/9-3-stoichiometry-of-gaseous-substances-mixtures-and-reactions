                                          <mi>n</mi>
                                          <mrow>
                                            <msub>
                                              <mi>N</mi>
                                              <mn>2</mn>
                                            </msub>
                                          </mrow>
                                        </msub>
                                      </mrow>
                                      <mrow>
                                        <msub>
