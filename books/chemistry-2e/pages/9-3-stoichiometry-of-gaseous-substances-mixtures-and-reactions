                                        <msub>
                                          <mi>n</mi>
                                          <mrow>
                                            <mi>T</mi>
                                            <mi>o</mi>
                                            <mi>t</mi>
                                            <mi>a</mi>
                                            <mi>l</mi>
                                          </mrow>
                                        </msub>
                                      </mrow>
                                    </mfrac>
                                    <mspace width="0.2em"/>
                                    <mo>=</mo>
                                    <mspace width="0.2em"/>
                                    <mfrac>
                                      <mrow>
                                        <mtext>2.83 mol</mtext>
                                      </mrow>
                                      <mrow>
                                        <mrow>
                                          <mo>(</mo>
                                          <mrow>
                                            <mn>2.83</mn>
                                            <mo>+</mo>
                                            <mn>8.41</mn>
                                          </mrow>
                                          <mo>)</mo>
                                        </mrow>
                                        <mspace width="0.2em"/>
                                        <mtext>mol</mtext>
                                      </mrow>
                                    </mfrac>
                                    <mspace width="0.2em"/>
                                    <mo>=</mo>
                                    <mn>0.252</mn>
                                  </mrow>
                                </mrow>
                                <annotation-xml encoding="MathML-Content">
                                  <mrow>
                                    <msub>
                                      <mi>X</mi>
                                      <mrow>
                                        <msub>
                                          <mi>O</mi>
                                          <mn>2</mn>
                                        </msub>
                                      </mrow>
                                    </msub>
                                    <mo>=</mo>
                                    <mspace width="0.2em"/>
                                    <mfrac>
                                      <mrow>
                                        <msub>
                                          <mi>n</mi>
                                          <mrow>
                                            <msub>
                                              <mi>O</mi>
                                              <mn>2</mn>
                                            </msub>
                                          </mrow>
                                        </msub>
                                      </mrow>
                                      <mrow>
                                        <msub>
                                          <mi>n</mi>
                                          <mrow>
                                            <mi>T</mi>
