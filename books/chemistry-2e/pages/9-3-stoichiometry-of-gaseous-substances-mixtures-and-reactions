                                    <mi>g</mi>
                                    <mo stretchy="false">)</mo>
                                    <mo>+</mo>
                                    <mn>3</mn>
                                    <msub>
                                      <mtext>H</mtext>
                                      <mn>2</mn>
                                    </msub>
                                    <mo stretchy="false">(</mo>
                                    <mi>g</mi>
                                    <mo stretchy="false">)</mo>
                                    <mspace width="0.2em"/>
                                    <mo stretchy="false">⟶</mo>
                                    <mspace width="0.2em"/>
                                    <mn>2</mn>
                                    <msub>
                                      <mrow>
                                        <mtext>NH</mtext>
                                      </mrow>
