                                    <mspace width="0.2em"/>
                                    <mtext>192 kPa</mtext>
                                    <mo>=</mo>
                                    <mtext>48.4 kPa</mtext>
                                  </mrow>
                                </mrow>
                                <annotation-xml encoding="MathML-Content">
                                  <mrow>
                                    <msub>
                                      <mi>P</mi>
                                      <mrow>
                                        <msub>
                                          <mi>O</mi>
                                          <mn>2</mn>
                                        </msub>
                                      </mrow>
                                    </msub>
                                    <mo>=</mo>
                                    <msub>
                                      <mi>X</mi>
                                      <mrow>
                                        <msub>
                                          <mi>O</mi>
                                          <mn>2</mn>
                                        </msub>
                                      </mrow>
                                    </msub>
                                    <mspace width="0.2em"/>
                                    <mo>×</mo>
                                    <mspace width="0.2em"/>
                                    <msub>
                                      <mi>P</mi>
                                      <mrow>
                                        <mi>T</mi>
