                                      <mrow>
                                        <mn>10</mn>
                                      </mrow>
                                      <mn>3</mn>
                                    </msup>
                                    <mspace width="0.2em"/>
                                    <mtext>billion</mtext>
                                    <mspace width="0.2em"/>
                                    <msup>
