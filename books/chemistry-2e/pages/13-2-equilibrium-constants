                                              <mo>]</mo>
                                            </mrow>
                                          </mrow>
                                          <mrow>
                                            <mrow>
                                              <mo stretchy="false">[</mo>
                                              <mrow>
                                                <msub>
                                                  <mtext>I</mtext>
                                                  <mn>2</mn>
                                                </msub>
                                              </mrow>
                                              <mo stretchy="false">]</mo>
                                            </mrow>
                                            <mrow>
                                              <mo stretchy="false">[</mo>
                                              <mrow>
                                                <msup>
                                                  <mtext>I</mtext>
                                                  <mtext>−</mtext>
                                                </msup>
                                              </mrow>
                                              <mo stretchy="false">]</mo>
                                            </mrow>
                                          </mrow>
                                        </mfrac>
                                      </mtd>
                                    </mtr>
                                    <mtr>
                                      <mtd columnalign="right">
                                        <mtext>HF</mtext>
                                        <mo stretchy="false">(</mo>
                                        <mi>a</mi>
                                        <mi>q</mi>
                                        <mo stretchy="false">)</mo>
                                        <mo>+</mo>
                                        <msub>
                                          <mtext>H</mtext>
                                          <mn>2</mn>
                                        </msub>
                                        <mtext>O</mtext>
                                        <mo stretchy="false">(</mo>
                                        <mi>l</mi>
                                        <mo stretchy="false">)</mo>
                                      </mtd>
                                      <mtd columnalign="left">
                                        <mo stretchy="false">⇌</mo>
                                      </mtd>
                                      <mtd columnalign="left">
                                        <msub>
                                          <mtext>H</mtext>
