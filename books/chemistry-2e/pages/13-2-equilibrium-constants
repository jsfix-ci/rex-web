                                    </msub>
                                    <msup>
                                      <mrow>
                                        <mo stretchy="false">]</mo>
                                      </mrow>
                                      <mn>2</mn>
                                    </msup>
                                  </mrow>
                                  <mrow>
                                    <mo stretchy="false">[</mo>
                                    <msub>
                                      <mtext>SO</mtext>
