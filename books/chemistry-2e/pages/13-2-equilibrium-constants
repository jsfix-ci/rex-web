                                    </mrow>
                                    <mrow>
                                      <msup>
                                        <mrow>
                                          <mo stretchy="false">[</mo>
                                          <msub>
                                            <mtext>NH</mtext>
                                            <mn>3</mn>
                                          </msub>
                                          <mo stretchy="false">]</mo>
                                        </mrow>
                                        <mn>4</mn>
                                      </msup>
                                      <mo stretchy="false">[</mo>
                                      <msub>
                                        <mrow>
                                          <mtext>O</mtext>
                                        </mrow>
                                        <mn>2</mn>
                                      </msub>
                                      <msup>
                                        <mrow>
                                          <mo stretchy="false">]</mo>
                                        </mrow>
                                        <mn>7</mn>
                                      </msup>
                                    </mrow>
                                  </mfrac>
                                </mrow>
                              </annotation-xml>
                            </semantics>
                          </math>
                        </p>
