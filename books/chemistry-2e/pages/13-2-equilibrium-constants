                                            </mrow>
                                          </mrow>
                                          <mrow>
                                            <mrow>
                                              <mo>[</mo>
                                              <mrow>
                                                <msub>
                                                  <mrow>
                                                    <mtext>NH</mtext>
                                                  </mrow>
                                                  <mn>3</mn>
                                                </msub>
                                              </mrow>
                                              <mo>]</mo>
                                            </mrow>
                                          </mrow>
                                        </mfrac>
