                                        <mi>g</mi>
                                        <mo stretchy="false">)</mo>
                                        <mo>+</mo>
                                        <menclose notation="horizontalstrike">
                                          <mrow>
                                            <mn>3</mn>
                                            <msub>
                                              <mtext>H</mtext>
                                              <mn>2</mn>
                                            </msub>
                                            <mo stretchy="false">(</mo>
                                            <mi>g</mi>
                                            <mo stretchy="false">)</mo>
                                          </mrow>
                                        </menclose>
                                        <mo>+</mo>
                                        <mn>3</mn>
                                        <msub>
                                          <mtext>I</mtext>
                                          <mtext>2</mtext>
                                        </msub>
                                        <mo stretchy="false">(</mo>
                                        <mi>g</mi>
                                        <mo stretchy="false">)</mo>
                                        <mo stretchy="false">⇌</mo>
