                                    </msup>
                                  </mrow>
                                  <mrow>
                                    <mo stretchy="false">[</mo>
                                    <msub>
                                      <mtext>SO</mtext>
                                      <mn>2</mn>
                                    </msub>
                                    <msup>
                                      <mrow>
                                        <mo stretchy="false">]</mo>
                                      </mrow>
                                      <mn>2</mn>
                                    </msup>
                                    <mspace width="0.2em"/>
                                    <mo stretchy="false">[</mo>
