                        <div data-type="equation" id="fs-idp66307536">
                          <math display="block">
                            <semantics>
                              <mrow>
                                <mrow>
                                  <mo>=</mo>
                                  <msub>
                                    <mi>K</mi>
                                    <mi>c</mi>
                                  </msub>
                                  <msup>
                                    <mrow>
                                      <mo stretchy="false">(</mo>
                                      <mi>R</mi>
                                      <mi>T</mi>
                                      <mo stretchy="false">)</mo>
                                    </mrow>
                                    <mrow>
                                      <mtext>Δ</mtext>
                                      <mi>n</mi>
                                    </mrow>
                                  </msup>
                                </mrow>
                              </mrow>
                              <annotation-xml encoding="MathML-Content">
                                <mrow>
                                  <mo>=</mo>
                                  <msub>
                                    <mi>K</mi>
                                    <mi>c</mi>
                                  </msub>
