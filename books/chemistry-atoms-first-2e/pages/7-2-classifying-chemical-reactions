                                  <mo stretchy="false">)</mo>
                                  <mspace width="0.2em"/>
                                  <mo stretchy="false">⟶</mo>
                                  <mspace width="0.2em"/>
                                  <mtext>Cu</mtext>
                                  <mspace width="0.2em"/>
                                  <msub>
                                    <mrow>
                                      <mo stretchy="false">(</mo>
