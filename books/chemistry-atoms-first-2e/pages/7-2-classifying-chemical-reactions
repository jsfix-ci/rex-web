                                      </mrow>
                                      <mn>3</mn>
                                    </msub>
                                    <msup>
                                      <mrow/>
                                      <mtext>−</mtext>
                                    </msup>
                                    <mo stretchy="false">)</mo>
                                    <mo>.</mo>
                                  </mrow>
                                </mrow>
