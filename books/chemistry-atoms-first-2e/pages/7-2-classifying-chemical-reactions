                                    <mrow>
                                      <mtext>OH</mtext>
                                    </mrow>
                                    <mtext>−</mtext>
                                  </msup>
                                  <mo stretchy="false">(</mo>
                                  <mi>a</mi>
                                  <mi>q</mi>
                                  <mo stretchy="false">)</mo>
                                </mrow>
                              </mrow>
                              <annotation-xml encoding="MathML-Content">
                                <mrow>
                                  <msub>
