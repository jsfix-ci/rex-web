                                            <msub>
                                              <mtext>H</mtext>
                                              <mn>2</mn>
                                            </msub>
                                            <mtext>O</mtext>
                                          </mrow>
                                        </mrow>
                                        <annotation-xml
                                          encoding="MathML-Content"
                                        >
                                          <mrow>
                                            <mn>6</mn>
