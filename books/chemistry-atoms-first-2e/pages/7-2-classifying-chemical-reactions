                                  <msub>
                                    <mrow>
                                      <mtext>NH</mtext>
                                    </mrow>
                                    <mn>4</mn>
                                  </msub>
                                  <msup>
                                    <mrow/>
                                    <mtext>+</mtext>
                                  </msup>
