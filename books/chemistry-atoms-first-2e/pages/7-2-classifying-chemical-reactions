                                            <mspace width="0.2em"/>
                                            <msup>
                                              <mrow>
                                                <mtext>Fe</mtext>
                                              </mrow>
                                              <mrow>
                                                <mtext>3+</mtext>
                                              </mrow>
                                            </msup>
                                          </mrow>
                                        </annotation-xml>
                                      </semantics>
                                    </math>
                                  </div>
                                  <div
                                    data-type="equation"
