                                        </mrow>
                                      </msup>
                                      <mo>+</mo>
                                      <msup>
                                        <mrow>
                                          <mtext>Fe</mtext>
                                        </mrow>
                                        <mrow>
                                          <mtext>3+</mtext>
                                        </mrow>
                                      </msup>
                                    </mrow>
                                  </mrow>
