                                      <mrow/>
                                      <mo>+</mo>
                                    </msup>
                                    <mo>+</mo>
                                    <msub>
                                      <mtext>H</mtext>
                                      <mn>2</mn>
                                    </msub>
                                    <mtext>O</mtext>
                                  </mrow>
                                </mrow>
