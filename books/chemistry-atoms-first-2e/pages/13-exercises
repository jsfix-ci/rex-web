                                          <mi>K</mi>
                                          <mi>P</mi>
                                        </msub>
                                        <mo>=</mo>
                                        <mn>2.2</mn>
                                        <mspace width="0.2em"/>
                                        <mo>×</mo>
                                        <mspace width="0.2em"/>
                                        <msup>
                                          <mrow>
                                            <mn>10</mn>
                                          </mrow>
                                          <mn>−6</mn>
                                        </msup>
                                      </mrow>
                                    </mrow>
                                    <annotation-xml encoding="MathML-Content">
                                      <mrow>
                                        <mn>2</mn>
                                        <msub>
