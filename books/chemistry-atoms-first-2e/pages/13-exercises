                                        </msub>
                                        <mtext>S</mtext>
                                        <mo stretchy="false">(</mo>
                                        <mi>g</mi>
                                        <mo stretchy="false">)</mo>
                                        <mo stretchy="false">⇌</mo>
                                        <mn>2</mn>
