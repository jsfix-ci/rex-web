                                  <mtext>Δ</mtext>
                                  <mi>H</mi>
                                  <mtext>°</mtext>
                                  <mo>=</mo>
                                  <mtext>−1366.8 kJ</mtext>
                                </mrow>
                              </mrow>
                              <annotation-xml encoding="MathML-Content">
                                <mrow>
                                  <msub>
                                    <mtext>C</mtext>
