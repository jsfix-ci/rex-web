                                    <mspace width="0.2em"/>
                                    <mtext>graphite</mtext>
                                    <mo stretchy="false">)</mo>
                                    <mo>+</mo>
                                    <mn>3</mn>
                                    <msub>
                                      <mtext>H</mtext>
                                      <mn>2</mn>
                                    </msub>
                                    <mo stretchy="false">(</mo>
                                    <mi>g</mi>
                                    <mo stretchy="false">)</mo>
                                    <mo>+</mo>
                                    <mspace width="0.1em"/>
                                    <mfrac>
                                      <mn>1</mn>
                                      <mn>2</mn>
                                    </mfrac>
                                    <msub>
                                      <mtext>O</mtext>
                                      <mn>2</mn>
                                    </msub>
                                    <mo stretchy="false">(</mo>
                                    <mi>g</mi>
                                    <mo stretchy="false">)</mo>
                                    <mspace width="0.2em"/>
                                    <mo stretchy="false">⟶</mo>
                                    <mspace width="0.2em"/>
                                    <msub>
                                      <mtext>C</mtext>
                                      <mn>2</mn>
                                    </msub>
                                    <msub>
                                      <mtext>H</mtext>
                                      <mn>5</mn>
                                    </msub>
