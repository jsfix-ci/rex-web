                                    <mspace width="3em"/>
                                    <mtext>Δ</mtext>
                                    <msubsup>
                                      <mi>H</mi>
                                      <mn>3</mn>
                                      <mo>°</mo>
                                    </msubsup>
                                    <mo>=</mo>
                                    <mn>−414.8</mn>
                                    <mspace width="0.2em"/>
                                    <mtext>kJ</mtext>
                                    <mspace width="0.2em"/>
                                    <mrow>
                                      <mo stretchy="false">[</mo>
                                      <mrow>
                                        <mn>2</mn>
                                        <mspace width="0.2em"/>
                                        <mo>×</mo>
                                        <mspace width="0.2em"/>
                                        <mtext>Δ</mtext>
                                        <msubsup>
                                          <mi>H</mi>
                                          <mtext>f</mtext>
                                          <mo>°</mo>
                                        </msubsup>
                                        <mo stretchy="false">(</mo>
                                        <msub>
                                          <mrow>
                                            <mtext>HNO</mtext>
                                          </mrow>
                                          <mn>3</mn>
                                        </msub>
                                        <mo stretchy="false">)</mo>
                                        <mo stretchy="false">]</mo>
                                      </mrow>
                                    </mrow>
                                  </mrow>
                                </annotation-xml>
                              </semantics>
                            </math>
                          </div>
                          <div data-type="equation" id="fs-idp194526576">
                            <math display="block">
                              <semantics>
                                <mrow>
                                  <mrow>
                                    <mfrac>
                                      <mn>1</mn>
                                      <mn>2</mn>
                                    </mfrac>
                                    <msub>
                                      <mtext>N</mtext>
                                      <mn>2</mn>
                                    </msub>
                                    <mo stretchy="false">(</mo>
                                    <mi>g</mi>
                                    <mo stretchy="false">)</mo>
