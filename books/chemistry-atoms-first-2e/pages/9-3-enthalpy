                                  <mo stretchy="false">⟶</mo>
                                  <mspace width="0.2em"/>
                                  <mn>2</mn>
                                  <msub>
                                    <mrow>
                                      <mtext>CO</mtext>
                                    </mrow>
                                    <mn>2</mn>
                                  </msub>
                                  <mo>+</mo>
