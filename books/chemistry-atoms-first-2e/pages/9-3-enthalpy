                                    <mo stretchy="false">(</mo>
                                    <mn>205.6</mn>
                                    <mo stretchy="false">)</mo>
                                    <mo>=</mo>
                                    <mtext>+102.8 kJ</mtext>
                                  </mrow>
                                </mrow>
