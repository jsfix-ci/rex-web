                                    <mspace width="0.2em"/>
                                    <mtext>kJ</mtext>
                                  </mrow>
                                </annotation-xml>
                              </semantics>
                            </math>
                          </div>
                          <p id="fs-idp186495680">
                            Therefore,
                            <math display="inline">
                              <semantics>
                                <mrow>
                                  <mrow>
                                    <mtext>Δ</mtext>
                                    <msubsup>
                                      <mi>H</mi>
                                      <mtext>f</mtext>
                                      <mo>°</mo>
                                    </msubsup>
                                    <mrow>
                                      <mo stretchy="false">[</mo>
                                      <mrow>
