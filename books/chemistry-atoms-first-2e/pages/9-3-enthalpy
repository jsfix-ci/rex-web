                                    </mfrac>
                                    <msub>
                                      <mtext>O</mtext>
                                      <mn>2</mn>
                                    </msub>
                                    <mo stretchy="false">(</mo>
                                    <mi>g</mi>
                                    <mo stretchy="false">)</mo>
                                    <mspace width="0.2em"/>
                                    <mo stretchy="false">⟶</mo>
                                    <mspace width="0.2em"/>
                                    <mtext>NO</mtext>
                                    <mo stretchy="false">(</mo>
