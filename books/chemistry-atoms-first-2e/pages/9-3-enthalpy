                                        <mo stretchy="false">)</mo>
                                        <mo stretchy="false">]</mo>
                                      </mrow>
                                    </mrow>
                                  </mrow>
                                </mrow>
                                <annotation-xml encoding="MathML-Content">
                                  <mrow>
                                    <mfrac>
                                      <mn>1</mn>
                                      <mn>2</mn>
                                    </mfrac>
                                    <msub>
                                      <mtext>N</mtext>
                                      <mn>2</mn>
                                    </msub>
                                    <mo stretchy="false">(</mo>
