                                        <mtext>kJ</mtext>
                                        <mo stretchy="false">)</mo>
                                        <mo>+</mo>
                                        <mo stretchy="false">(</mo>
                                        <mtext>+285.8 kJ</mtext>
                                        <mo stretchy="false">)</mo>
                                        <mo>+</mo>
                                        <mo stretchy="false">(</mo>
                                        <mn>−414.8</mn>
                                        <mspace width="0.2em"/>
                                        <mtext>kJ</mtext>
                                        <mo stretchy="false">)</mo>
                                        <mo>+</mo>
                                        <mo stretchy="false">(</mo>
                                        <mtext>+90.2 kJ</mtext>
                                        <mo stretchy="false">)</mo>
                                      </mtd>
                                    </mtr>
                                    <mtr>
                                      <mtd/>
                                      <mtd columnalign="left">
                                        <mo>=</mo>
                                        <mn>−138.4</mn>
                                        <mspace width="0.2em"/>
                                        <mtext>kJ</mtext>
                                      </mtd>
                                    </mtr>
                                  </mtable>
                                </mrow>
                                <annotation-xml encoding="MathML-Content">
                                  <mtable>
                                    <mtr>
                                      <mtd columnalign="right">
                                        <mtext>Δ</mtext>
                                        <msubsup>
                                          <mi>H</mi>
                                          <mrow>
                                            <mtext>rxn</mtext>
                                          </mrow>
                                          <mo>°</mo>
