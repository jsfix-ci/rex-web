                                  <mo>=</mo>
                                  <mn>7.52</mn>
                                </mrow>
                              </mrow>
                              <annotation-xml encoding="MathML-Content">
                                <mrow>
                                  <mn>0.0587</mn>
                                  <mspace width="0.2em"/>
