                                  <mo stretchy="false">)</mo>
                                  <mspace width="0.2em"/>
                                  <mo stretchy="false">⟶</mo>
                                  <mspace width="0.2em"/>
                                  <mtext>CO</mtext>
                                  <mo stretchy="false">(</mo>
                                  <mi>g</mi>
                                  <mo stretchy="false">)</mo>
                                  <mspace width="3em"/>
                                  <mtext>Δ</mtext>
                                  <mi>H</mi>
                                  <mtext>°</mtext>
                                  <mo>=</mo>
                                  <mn>−111</mn>
