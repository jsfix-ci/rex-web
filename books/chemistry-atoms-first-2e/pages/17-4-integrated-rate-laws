                                            <mn>2</mn>
                                          </msub>
                                          <mspace width="0.2em"/>
                                          <mo stretchy="false">⟶</mo>
                                          <mspace width="0.2em"/>
                                          <msub>
                                            <mtext>2H</mtext>
                                            <mn>2</mn>
                                          </msub>
                                          <mtext>O</mtext>
