                                              <mo stretchy="false">(</mo>
                                              <mn>0.200</mn>
                                              <mspace width="0.2em"/>
                                              <mtext>mol</mtext>
                                              <mspace width="0.2em"/>
                                              <msup>
                                                <mtext>L</mtext>
                                                <mn>−1</mn>
                                              </msup>
                                              <mo stretchy="false">)</mo>
