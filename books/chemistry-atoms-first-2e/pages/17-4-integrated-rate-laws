                                        <mfrac>
                                          <mrow>
                                            <mrow>
                                              <mo>[</mo>
                                              <mi>x</mi>
                                              <mo>]</mo>
                                            </mrow>
                                          </mrow>
                                          <mrow>
                                            <mrow>
                                              <mo>[</mo>
                                              <mrow>
                                                <mn>0.200</mn>
