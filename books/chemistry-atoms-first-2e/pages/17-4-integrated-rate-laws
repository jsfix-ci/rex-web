                                            <mrow>
                                              <mn>1</mn>
                                              <mtext>/</mtext>
                                              <mn>2</mn>
                                            </mrow>
                                          </msub>
                                        </mtd>
                                        <mtd columnalign="left">
                                          <mo>=</mo>
                                        </mtd>
                                        <mtd columnalign="left">
                                          <mfrac>
                                            <mn>1</mn>
                                            <mrow>
                                              <mo stretchy="false">[</mo>
                                              <mo stretchy="false">(</mo>
                                              <mn>0.0576</mn>
                                              <mspace width="0.2em"/>
                                              <mtext>L</mtext>
                                              <mspace width="0.2em"/>
                                              <msup>
                                                <mtext>mol</mtext>
