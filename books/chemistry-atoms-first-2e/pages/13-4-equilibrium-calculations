                                    <mrow>
                                      <mo>(</mo>
                                      <mrow>
                                        <mo>−</mo>
                                        <mspace width="0.1em"/>
                                        <mfrac>
                                          <mrow>
                                            <mn>55.7</mn>
                                            <mspace width="0.2em"/>
                                            <mo>×</mo>
                                            <mspace width="0.2em"/>
                                            <msup>
                                              <mrow>
                                                <mn>10</mn>
                                              </mrow>
                                              <mn>3</mn>
                                            </msup>
                                            <mspace width="0.2em"/>
                                            <mtext>J/mol</mtext>
                                          </mrow>
