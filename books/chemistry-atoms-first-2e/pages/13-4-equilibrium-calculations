                                  <mi>K</mi>
                                  <mo>=</mo>
                                  <msup>
                                    <mi>e</mi>
                                    <mrow>
                                      <mo>−</mo>
                                      <mspace width="0.1em"/>
                                      <mfrac>
                                        <mrow>
                                          <mtext>Δ</mtext>
                                          <mi>G</mi>
                                          <mtext>°</mtext>
                                        </mrow>
                                        <mrow>
                                          <mi>R</mi>
