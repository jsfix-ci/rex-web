                                  <mtext>at equilibrium</mtext>
                                  <mo stretchy="false">)</mo>
                                </mrow>
                              </annotation-xml>
                            </semantics>
                          </math>
                        </div>
                        <div data-type="equation" id="fs-idm55951760">
                          <math display="block">
                            <semantics>
                              <mrow>
                                <mrow>
                                  <mtext>Δ</mtext>
                                  <mi>G</mi>
                                  <mtext>°</mtext>
                                  <mo>=</mo>
