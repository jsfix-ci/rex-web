                                    </msup>
                                    <mo stretchy="false">(</mo>
                                    <mi>a</mi>
                                    <mi>q</mi>
                                    <mo stretchy="false">)</mo>
                                    <mo>+</mo>
                                    <msup>
                                      <mrow>
