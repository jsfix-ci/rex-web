                                        <mrow>
                                          <mo>[</mo>
                                          <mrow>
                                            <msub>
                                              <mtext>O</mtext>
                                              <mn>2</mn>
                                            </msub>
                                          </mrow>
                                          <mo>]</mo>
                                        </mrow>
                                      </mrow>
                                    </msqrt>
                                  </mrow>
                                </mrow>
                                <annotation-xml encoding="MathML-Content">
                                  <mrow>
                                    <mrow>
                                      <mo>[</mo>
                                      <mrow>
                                        <mtext>NO</mtext>
                                      </mrow>
                                      <mo>]</mo>
                                    </mrow>
                                    <mo>=</mo>
                                    <mspace width="0.2em"/>
                                    <msqrt>
                                      <mrow>
                                        <msub>
                                          <mi>K</mi>
                                          <mi>c</mi>
                                        </msub>
                                        <mrow>
                                          <mo>[</mo>
