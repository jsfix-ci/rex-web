                                <mover>
                                  <mo stretchy="false">⇌</mo>
                                  <mrow/>
                                </mover>
                                <msub>
                                  <mtext>N</mtext>
                                  <mn>2</mn>
                                </msub>
                                <mo stretchy="false">(</mo>
                                <mi>g</mi>
                                <mo stretchy="false">)</mo>
                                <mo>+</mo>
