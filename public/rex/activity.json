{
  "management": {
    "activities": {
      "autoSearch": [
        {
          "id": {"fn": "uuid"},
          "title": "Reading",
          "description": "read",
          "integration": "rex",
          "type": "reading",
          "launchParams": {
            "book": {"fn": "value", "name": "scope.id"},
            "section": {
              "fn": "map",
              "array": {"fn": "value", "name": "resources"},
              "map": {"fn": "value", "name": "id"}
            }
          }
        }
      ]
    },
    "support":{
      "autoSearch": [
        {
          "url": {
            "fn": "template",
            "template": "/books/[slug]/pages/[default_page_slug]",
            "parameters": {
              "[slug]": {"fn": "value", "name": "slug"},
              "[default_page_slug]": {"fn": "value", "name": "default_page.slug"}
            }
          },
          "icon": "/static/icons/book.svg",
          "title": "My Textbook",
          "description": "Browse the textbook."
        },
        {
          "url": {
            "fn": "template",
            "template": "/books/[slug]/pages/[default_page_slug]?modal=MH",
            "parameters": {
              "[slug]": {"fn": "value", "name": "slug"},
              "[default_page_slug]": {"fn": "value", "name": "default_page.slug"}
            }
          },
          "icon": "/static/icons/highlights.svg",
          "title": "My Highlights",
          "description": "Review the notes and highlights made during reading assignments."
        }
      ]
    }
  },
  "activities":{
    "reading":{
      "launch":{"url":"/apps/rex/assigned/:id","navigation":true,"xapi":false},
      "objectIRI":"https://openstax.org/orn/reading/:id",
      "objectTypeIRI":"http://id.tincanapi.com/activitytype/school-assignment"
    },
    "youtube_video":{
      "launch":{"url":"https://www.youtube.com/embed/:id","navigation":false,"xapi":false},
      "objectIRI":"https://openstax.org/orn/video/youtube/:id",
      "objectTypeIRI":"http://adlnet.gov/expapi/activities/media"
    }
  }
}
